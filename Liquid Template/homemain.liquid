          </div>
        </div>
      </div>

      <!--Colorful Banner Section-->
      <div
        class="container-fluid"
        style="
          padding-top: 40px;
          padding-bottom: 40px;
          outline: 2px solid red;
          outline-offset: 4px;
          background: linear-gradient(94.88deg, #e95252 16.28%, #ec5582 81.93%);
          margin-bottom: 5px;
        "
      >
        <div class="row" id="centeredRow">
          <div class="col-sm-6">
            <div
              class="row"
              id="centeredRow"
              style="padding-left: 10px; padding-right: 10px"
            >
              <p>
                {{ campaign | term_by_name: "Cstm2" }}
              </p>
            </div>
          </div>
        </div>
      </div>


<div class="aboutSectionContainer">
  <div class="row">
    <div class="container-fluid">
      <a class="anchor" id="about"></a>
      <div
        class="container-fluid"
        style="padding-top: 50px; padding-bottom: 50px; background-color: white"
      >
        <div class="row" id="centeredRow">
          <h3 class="abtTitle">About the Campaign</h3>
        </div>

        <div class="row" id="centeredRow">
          <div
            style="
              column-count: 2;
              column-gap: 40px;
              width: 90%;
              column-width: 400px;
              margin-left: 20px;
              margin-right: 20px;
            "
          >
            <p>
              {{ campaign | term_by_name: "Cstm1" }}
            </p>
          </div>
        </div>
      </div>

      <!--Colorful Banner Section-->
      <div
        class="container-fluid"
        style="
          padding-top: 40px;
          padding-bottom: 40px;
          outline: 2px solid red;
          outline-offset: 4px;
          background: linear-gradient(94.88deg, #e95252 16.28%, #ec5582 81.93%);
          color: white;
          text-align: center;
        "
      >
        <div class="row" id="centeredRow">
          <div class="col-sm-6">
            <div
              class="row"
              id="centeredRow"
              style="padding-left: 10px; padding-right: 10px"
            >
              <p>
                {{ campaign | term_by_name: "Cstm2" }}
              </p>
            </div>
          </div>
        </div>
      </div>


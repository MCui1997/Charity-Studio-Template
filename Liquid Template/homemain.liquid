<div class="aboutSectionContainer">
  <div class="row">
    <div class="container-fluid">
      <a class="anchor" id="about"></a>
      <div
        class="container-fluid"
        style="padding-top: 50px; padding-bottom: 50px; background-color: white"
      >
        <div class="row" id="centeredRow">
          <h3 class="abtTitle">About the Campaign</h3>
        </div>

        <div class="row" id="centeredRow">
          <div class="aboutRows">
            <p style>
              {{ campaign | term_by_name: "Cstm1" }}
